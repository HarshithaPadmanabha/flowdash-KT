# Copy this file to .env and fill in your values.
# Do not commit .env to version control.

# Database (PostgreSQL via Supabase or other)
DATABASE_URL=postgresql://user:password@host:5432/dbname?pgbouncer=true
DIRECT_URL=postgresql://user:password@host:5432/dbname

# Auth
JWT_SECRET=your-long-random-secret-at-least-32-chars
BCRYPT_ROUNDS=10

# Optional: Supabase (if used elsewhere)
SUPABASE_URL=
SUPABASE_ANON_KEY=

NODE_ENV=development

# Keycloak (required for login)
KEYCLOAK_BASE_URL=http://localhost:8081
KEYCLOAK_REALM=your-realm-name
# Use either KEYCLOAK_CLIENT_ID / KEYCLOAK_CLIENT_SECRET or KEYCLOAK_PROVISIONER_*
KEYCLOAK_CLIENT_ID=backend-auth
KEYCLOAK_CLIENT_SECRET=your-client-secret

# HRM redirect (optional)
HRM_BACKEND_ROUTE=
